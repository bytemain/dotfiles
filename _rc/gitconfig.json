{
  "pull": {
    "rebase": "true"
  },
  "alias": {
    "s": "status",
    "sb": "status -sb",
    "d": "diff",
    "co": "checkout",
    "cpk": "cherry-pick",
    "br": "branch",
    "last": "log -1 HEAD",
    "cane": "commit --amend --no-edit",
    "pr": "pull --rebase",
    "lo": "log --oneline -n 10",
    "a": "add .",
    "cm": "commit -m",
    "cam": "commit --amend -m",
    "rh": "reset --hard",
    "f": "fetch",
    "up": "upstream",
    "upstream": "!git push -u origin HEAD",
    "wt": "worktree",
    "gt": "describe --abbrev=0",
    "psuh": "push",
    "rhh": "reset --hard HEAD^",
    "rsh": "reset --soft HEAD^"
  },
  "core": {
    "excludesfile": "~/.gitignore_global",
    "autocrlf": "false",
    "filemode": "false"
  },
  "color": {
    "ui": "true"
  }
}