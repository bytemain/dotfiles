{
    "coc.preferences.jumpCommand": "tab drop",
    "languageserver": {
      "ccls": {
        "command": "ccls",
        "filetypes": ["c", "cc", "cpp", "c++", "objc", "objcpp"],
        "rootPatterns": [".ccls", "compile_commands.json", ".git/", ".hg/"],
        "initializationOptions": {
            "cache": {
              "directory": "/tmp/ccls"
            },
            "highlight": { "lsRanges" : true }
        }
      }
    }
}
